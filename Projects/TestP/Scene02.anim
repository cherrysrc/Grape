#relative to scene start
(5 50)
{
RectB fade 1.0
RectC fade 1.0
};
(50 150)
{
RectB move 400 300
RectC move 400 300
};
(150 250)
{
RectB rotate 90
RectC rotate -90
};
(250 350)
{
RectB rotate 180
RectC rotate -180
};
(350 450)
{
RectB rotate 270
RectC rotate -270
};
(450 550)
{
RectB rotate 360
RectC rotate -360
};
